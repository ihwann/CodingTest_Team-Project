<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.stevelee.java.dao.ReviewDao">
	<select id="select_all_review" resultType="com.stevelee.java.dto.ReviewDto">
